<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/layouts/">
	<excludes>
		<pattern>/static/*</pattern>
		<pattern>/api/*</pattern>
		<pattern>/resource/*</pattern>
		<pattern>/login</pattern>
		<pattern>/register</pattern>
		<pattern>/system/checkVersion/</pattern>
		<pattern>/admin/user/addSessionStyle</pattern>
		<pattern>/admin/user/json*</pattern>
		<pattern>/pe/getCityList</pattern>
	</excludes>
	
	<decorator name="default" page="main.jsp">
		<pattern>/*</pattern>
	</decorator>
</decorators>
